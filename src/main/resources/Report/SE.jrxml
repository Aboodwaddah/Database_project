    (SELECT COUNT(*) FROM public.sales) AS "Total Cars Sold",
    (SELECT COUNT(DISTINCT id_employee) FROM public.sales) AS "Total Employees Involved",
    (SELECT MAX(date_sales) FROM public.sales) AS "Most Recent Sale Date",
