		<queryString language="SQL">
			<![CDATA[SELECT
    e.id_expenses,
    e.expenses_date,
    e.amount,
    e.description,
    e.category,
    ex.exhibition_location,
    p.name AS employee_name
FROM
    public.expenses e
JOIN
    public.exhibition ex ON e.id_exhibition = ex.id_exhibition
JOIN
    public.employee emp ON e.id_employee = emp.id_employee
JOIN
    public.person p ON emp.id_person = p.id_person
ORDER BY
    e.expenses_date DESC;]]>
		</queryString>
